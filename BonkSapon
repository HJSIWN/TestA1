local function Translate(ThaiLang, EngLang)
	local Resut = ThaiLang
	if getgenv().TranslateEnv then
		if getgenv().TranslateEnv == "Thai" then
			Resut = ThaiLang
		elseif getgenv().TranslateEnv == "Eng" then
			Resut = EngLang
		end
	end
	return Resut
end

