local function Translate(ThaiLang, EngLang)
	local Resut = ThaiLang
	if getgenv().Setting and getgenv().Setting.Language then
		if getgenv().Setting.Language == "Thai" then
			Resut = ThaiLang
		elseif getgenv().Setting.Language == "English" then
			Resut = EngLang
		end
	end
	return Resut
end

